# Buat .gitignore yang lebih lengkap
echo "# Dependencies
/node_modules
/.pnp
.pnp.js

# Testing
/coverage

# Next.js
/.next/
/out/

# Production
/build

# Misc
.DS_Store
*.pem
Thumbs.db

# Debug
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Local env files
.env*.local

# Vercel
.vercel

# System files
AppData/
Desktop/
Documents/
Downloads/
Favorites/
Music/
Pictures/
Videos/
.vscode/
*.exe
ntuser.*" > .gitignore

# Commit .gitignore baru
git add .gitignore
git commit -m "Update .gitignore to exclude system files"