# Buat file .gitattributes
echo "* text=auto
*.js text eol=lf
*.jsx text eol=lf
*.ts text eol=lf
*.tsx text eol=lf
*.json text eol=lf
*.css text eol=lf
*.html text eol=lf
*.md text eol=lf" > .gitattributes

# Commit file .gitattributes
git add .gitattributes
git commit -m "Add .gitattributes to normalize line endings"